{
  "files": [],
  "references": [
    {
      "path": "./shared"
    },
    {
      "path": "./backend"
    },
    {
      "path": "./frontend"
    }
  ],
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
     "baseUrl": ".",
    "paths": {
      "@shared/*": ["shared/*"],
      "@backend/*": ["backend/*"],
      "@frontend/*": ["frontend/*"]
    }
  }
}